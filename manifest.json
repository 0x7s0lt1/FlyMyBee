{
    "manifest_version": 3,
    "name": "MyBee",
    "description": "MyBee",
    "version": "1.0",
    "permissions": ["scripting"],
    "action": {
      "default_icon": "/src/images/icon-active.png"
    },
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
        {
          "js": ["/src/js/content.js"],
          "matches": [
            "https://*/*"
          ]
        }
      ],
      "web_accessible_resources": [{
        "resources": ["src/images/buzzbee.gif"],
        "matches": ["https://*/*"]
      }]
  }